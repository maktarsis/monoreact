{
  "name": "monoreact-root",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Maksym Tarsis",
    "email": "tarsis.maksym@gmail.com",
    "url": "https://github.com/untaggable"
  },
  "workspaces": [
    "cli",
    "example-app/packages/*"
  ],
  "scripts": {
    "cpd": "jscpd **/src",
    "postinstall": "yarn build && yarn example:install",
    "example:install": "cd example-app && yarn install",
    "build": "cd cli && yarn build",
    "start": "cd cli && yarn start",
    "lint": "cd cli && yarn lint",
    "test": "cd cli && yarn test",
    "ci": "yarn install && yarn add ./cli --D -W && yarn test"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.1",
    "eslint-plugin-sonarjs": "0.10.0",
    "husky": "4.3.8",
    "jscpd": "3.4.1",
    "lint-staged": "11.2.6",
    "monoreact": "*",
    "prettier": "2.4.1",
    "webpack": "4.44.2",
    "yarn": "1.22.17"
  },
  "resolutions": {
    "tslib": "2.3.1"
  },
  "keywords": [
    "cli",
    "microfrontend",
    "workspaces",
    "submodules",
    "monorepo",
    "react",
    "typescript",
    "rollup"
  ]
}
