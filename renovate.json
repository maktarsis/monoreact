{
  "extends": ["config:base"],
  "baseBranches": ["master"],
  "branchPrefix": "bump/",
  "branchConcurrentLimit": 0,
  "enabled": true,

  "assignees": ["retarsis"],
  "packageRules": [
    {
      "matchPackageNames": ["react", "react-dom", "@types/react", "@types/react-dom"],
      "groupName": "React monorepo"
    },
    {
      "excludePackagePrefixes": ["@testing-library"]
    }
  ]
}
