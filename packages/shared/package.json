{
  "name": "@repo-warehouse/shared",
  "version": "0.0.16",
  "private": false,
  "main": "dist/static/js/main.chunk.js",
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "start:s": "react-app-rewired start",
    "build:s": "react-app-rewired build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "mkdir": "if not exist dist mkdir dist",
    "watch:s": "npm-watch build:s",
    "watch": "node watch.js",
    "watch1": "npx babel src/components --watch --presets=minify --out-dir dist --copy-files --no-copy-ignored --ignore \"src/**/*.spec.js\",\"src/**/*.test.js\""
  },
  "dependencies": {
    "@babel/cli": "~7.8.4",
    "@babel/preset-react": "~7.8.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "babel-preset-minify": "0.5.1",
    "npm-watch": "~0.6.0",
    "react-app-rewired": "~2.1.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
